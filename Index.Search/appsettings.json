{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RabbitMQ": {
    "Uri": "amqp://guest:guest@rabbitmq:5672",
    "ClientProvidedName": "Index",
    "ExchangeName": "MainExchange",
    "Queues": [
      {
        "Name": "Ad",
        "MessageType": "AdMessage"
      }
    ]
  },
  "ElasticSearch": {
    "Uri": "http://elasticsearch:9200",
    "DefaultIndex": "ads-elastic"
  },
  "AllowedHosts": "*"
}
